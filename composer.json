{
  "name": "exoticca/coding-style",
  "type": "library",
  "description": "PHP Coding Style rules for Exoticca's projects.",
  "keywords": ["Code style"],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/exoticcaengineering/php-coding-style/issues",
    "source": "https://github.com/exoticcaengineering/php-coding-style"
  },
  "require": {
    "symplify/easy-coding-standard": "^12.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "vimeo/psalm": "^5.24"
  },
  "autoload": {
    "psr-4": {
      "Exoticca\\": "src"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
